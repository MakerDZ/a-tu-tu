{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
      "build": {
          "dependsOn": ["^build"],
          "inputs": ["$TURBO_DEFAULT$", ".env*"],
          "outputs": [".next/**", "!.next/cache/**"]
      },
      "lint": {
          "dependsOn": ["^lint"]
      },
      "dev": {
          "cache": false,
          "persistent": true
      },
      "register": {
          "cache": false,
          "persistent": true
      },
      "unregister": {
          "cache": false,
          "persistent": true
      },
      "start": {
          "dependsOn": ["^build"],
          "inputs": ["$TURBO_DEFAULT$", ".env*"]
      }
  }
}
